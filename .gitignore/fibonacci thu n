/*fibonacci*/

#include <stdio.h>
#include <string.h>

int fibonacci(int n)
{
    if( n == 0 || n == 1)
        return 1;
    return fibonacci(n-1)+fibonacci(n-2);
}

int main(){
    int n = -1;
    while(n < 0){
        printf("Nhap n>=0: ");
        scanf("%d", &n);
    }
    printf("So fibonacci thu %d la %d", n, fibonacci(n));

    return 0;
}
