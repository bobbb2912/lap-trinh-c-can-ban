/*Tính giá trị n giai thừa (n!), với n nhập vào từ bàn phím. In kết quả ra màn hình.
*/

#include <stdio.h>
#include <math.h>


int main(){
    long long gt = 1;
    int n;
    printf("Nhap n: ");
    scanf("%d", &n);
    for(int i = 1; i <= n; i++){
        gt*=i;
    }
    printf("(%d!) = %lld", n, gt);
    return 0;
}
